<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="PatchOperationFindMod">
		<mods>
			<li>A Dog Said... Animal Prosthetics</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>



			<!-- Organ installing recipes == should be most animals -->
			<li Class="PatchOperationAdd"> <!-- "Inserting animal defNames into InstallBionicAnimalStomach surgery <recipeUsers>" -->
				<xpath>/Defs/RecipeDef[defName = "InstallBionicAnimalStomach"]/recipeUsers</xpath>
				<value>
					<li>GR_EldritchBear</li>
					<li>GR_EldritchBoomalope</li>
					<li>GR_EldritchChicken</li>
					<li>GR_EldritchMuffalo</li>
					<li>GR_EldritchReptile</li>
					<li>GR_EldritchThrumbo</li>
					<li>GR_EldritchWolf</li>
				</value>
			</li>

			<li Class="PatchOperationAdd"> <!-- "Inserting animal defNames into InstallBionicAnimalKidney surgery <recipeUsers>" -->
				<xpath>/Defs/RecipeDef[defName = "InstallBionicAnimalKidney"]/recipeUsers</xpath>
