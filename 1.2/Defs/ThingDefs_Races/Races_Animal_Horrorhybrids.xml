<Defs>

	<!-- Bear -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>GR_EldritchBear</defName>
		<label>bear young</label>
		<description>This otherworldly bear has repulsive, purple coloured tentacles protruding from its maw. Its pelt has a strange wooden-like appearance. Its ravenous appetite can only be sated by consuming large amounts of meat, which the creature dissolves with the green acidic goo dropping from its many gaping maws.\n\nUseful abilities: Since the creature constantly regenerates, its tentacles can be trimmed every few days, and used as a substitute for wood.</description>


		<statBases>
			<MoveSpeed>5</MoveSpeed>
			<ComfyTemperatureMin>-80</ComfyTemperatureMin>
			<MarketValue>1400</MarketValue>
			<MeatAmount>20</MeatAmount>

		</statBases>
		<tools>
			<li>
				<label>hooves</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>12</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>ROM_Hooves</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>19</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>tentacles</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>14</power>
				<cooldownTime>0.9</cooldownTime>
				<linkedBodyPartsGroup>ROM_Tentacles</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>19</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<capacities>
					<li>ToxicBite</li>
				</capacities>
				<power>12</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>18</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>6</power>
				<cooldownTime>1.65</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>

		<comps>

			<li Class="GeneticRim.CompProperties_AnimalProduct">
				<resourceDef>GR_DarkYoungWoodLog</resourceDef>
				<gatheringIntervalDays>10</gatheringIntervalDays>
				<resourceAmount>50</resourceAmount>
			</li>
			<li Class="GeneticRim.CompProperties_Draftable">
				<rage>true</rage>
				<horror>true</horror>
			</li>
		</comps>
		<race>
			<thinkTreeMain>GR_DraftableAnimal</thinkTreeMain>

			<useLeatherFrom>ROM_DarkYoung</useLeatherFrom>
			<useMeatFrom>Bear_Grizzly</useMeatFrom>
			<herdAnimal>true</herdAnimal>
			<predator>true</predator>
			<body>ROM_DarkYoungBody</body>
			<baseHungerRate>2</baseHungerRate>
			<baseBodySize>3.5</baseBodySize>
			<baseHealthScale>2.75</baseHealthScale>
			<foodType>CarnivoreAnimal</foodType>
			<gestationPeriodDays>50</gestationPeriodDays>
			<wildness>0.80</wildness>
			<lifeExpectancy>160</lifeExpectancy>
			<trainability>Advanced</trainability>
			<manhunterOnTameFailChance>0.018</manhunterOnTameFailChance>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
			<lifeStageAges>
				<li>
					<def>GR_ROM_Spawn</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>GR_ROM_FullyFormed</def>
					<minAge>0.8</minAge>
					<soundWounded>Pawn_ROM_DarkYoung_Wounded</soundWounded>
					<soundDeath>Pawn_ROM_DarkYoung_Death</soundDeath>
					<soundCall>Pawn_ROM_DarkYoung_Call</soundCall>
				</li>
				<li>
					<def>GR_ROM_Ancient</def>
					<minAge>10</minAge>
				</li>
				<li>
					<def>GR_ROM_Eldritch</def>
					<minAge>50</minAge>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
			<hediffGiverSets>
				<li>ROM_NonBleeder</li>
			</hediffGiverSets>
		</race>
		<tradeTags>
      <li>AnimalGenetic</li>
    </tradeTags>
		<butcherProducts>
			<ROM_RawCHFood_DarkYoungMeat>50</ROM_RawCHFood_DarkYoungMeat>
		</butcherProducts>
		<modExtensions>
			<li Class = "CosmicHorror.PawnExtension">
				<invisible>false</invisible>
				<regenRate>0.0125</regenRate>
				<regenInterval>1000</regenInterval>
				<sanityLossRate>0.0</sanityLossRate>
				<painFactor>0.25</painFactor>
			</li>
		</modExtensions>

	</ThingDef>
