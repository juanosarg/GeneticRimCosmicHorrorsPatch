	<!-- Boomalope -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>GR_EldritchBoomalope</defName>
		<label>formless boomalope of Tsathoggua</label>
		<description>This polymorphic entity is made of light brown ichor and looks like amorphous goo. It seems to be extremely resilient and very difficult to dispatch. Its pelt seems to be powerfully acidic in substance and can dissolve human flesh with even a slight touch.\n\nUseful abilities: Explodes on death with a very powerful psionic blast. Decent chemfuel production.</description>


		<statBases>
			<MoveSpeed>4</MoveSpeed>
			<ComfyTemperatureMin>-10</ComfyTemperatureMin>
			<MarketValue>1400</MarketValue>
			<MeatAmount>20</MeatAmount>

		</statBases>
		<comps>

			<li Class="GeneticRim.CompProperties_AnimalProduct">
				<resourceDef>Chemfuel</resourceDef>
				<gatheringIntervalDays>2</gatheringIntervalDays>
				<resourceAmount>22</resourceAmount>
				<customResourceString>ChemfuelProd</customResourceString>
			</li>
			<li Class="GeneticRim.CompProperties_Draftable">
				<explodable>true</explodable>
				<horror>true</horror>
			</li>
		</comps>  
		<tools>
			<li>
				<label>amoebic tentacle</label>

				<capacities>
					<li>GR_VeryToxicBite</li>
				</capacities>
				<power>9</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>12</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>13</power>
				<cooldownTime>1.65</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>


		<race>
			<deathActionWorkerClass>GeneticRim.DeathActionWorker_PsionicExplosion</deathActionWorkerClass>
			<leatherDef>GR_Shiftskin</leatherDef>
			<thinkTreeMain>GR_DraftableAnimal</thinkTreeMain>

			<useMeatFrom>Boomalope</useMeatFrom>
			<herdAnimal>true</herdAnimal>
			<predator>false</predator>
			<body>GR_Amorphous</body>
			<baseHungerRate>2</baseHungerRate>
			<baseBodySize>3.5</baseBodySize>
			<baseHealthScale>1</baseHealthScale>
			<foodType>VegetarianRoughAnimal</foodType>
			<gestationPeriodDays>47</gestationPeriodDays>
			<wildness>0.80</wildness>
			<lifeExpectancy>180</lifeExpectancy>
			<trainability>Intermediate</trainability>
			<manhunterOnTameFailChance>0.018</manhunterOnTameFailChance>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
			<lifeStageAges>
				<li>
					<def>GR_ROM_Spawn</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>GR_ROM_FullyFormed</def>
					<minAge>0.8</minAge>
					<soundWounded>Pawn_ROM_DarkYoung_Wounded</soundWounded>
					<soundDeath>Pawn_ROM_DarkYoung_Death</soundDeath>
					<soundCall>Pawn_ROM_DarkYoung_Call</soundCall>
				</li>
				<li>
					<def>GR_ROM_Ancient</def>
					<minAge>10</minAge>
				</li>
				<li>
					<def>GR_ROM_Eldritch</def>
					<minAge>45</minAge>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>

		</race>
		<tradeTags>
      <li>AnimalGenetic</li>
    </tradeTags>
		<butcherProducts>
			<GR_ROM_RawCHFood_FormlessMeat>50</GR_ROM_RawCHFood_FormlessMeat>
		</butcherProducts>
		<modExtensions>
			<li Class = "CosmicHorror.PawnExtension">
				<invisible>false</invisible>
				<regenRate>0.01</regenRate>
				<regenInterval>1000</regenInterval>
				<sanityLossRate>0.0</sanityLossRate>
				<painFactor>0.25</painFactor>
			</li>
		</modExtensions>

